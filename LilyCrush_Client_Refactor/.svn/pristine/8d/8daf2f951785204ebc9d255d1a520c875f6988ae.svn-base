package view;

import enums.Avatar;

public class ImgPathHelper {
public static int getAvatarPath(Avatar avatar){
	switch (avatar) {
	case defaultImg:
		return 0;
	case hokain1:
		return 1;
	case hokain2:
		return 2;
	case hokain3:
		return 3;
	case hokain4:
		return 4;
	case hokain5:
		return 5;

	default:
		return 0;
	}
}
}
