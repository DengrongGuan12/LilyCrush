package view.component;

import java.io.File;
import java.io.FilenameFilter;

/**
 *输出一个目录下的所有PNG文件的filter 
 * @author shen
 *
 */
public class PNGFilter implements FilenameFilter {

	@Override
	public boolean accept(File dir, String name) {
		if(name.endsWith(".png"))
			return true;
		else
			return false;
	}

}
