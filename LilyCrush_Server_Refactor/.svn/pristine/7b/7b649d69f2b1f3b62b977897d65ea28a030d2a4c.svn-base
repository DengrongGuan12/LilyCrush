package pos.historyPO;

import java.util.ArrayList;
import java.util.Date;

public class MultiGameHistoryItem {
	private Date date;
	private int eachPoint;
	private int maxCombo;
	private ArrayList<String> coworkerList;
	
	public MultiGameHistoryItem(Date date , int point,int maxCombo,ArrayList<String> coworkerList){
		this.date = date;
		this.eachPoint = point;
		this.maxCombo = maxCombo;
		this.coworkerList = coworkerList;
	}
	
	public Date getDate() {
		return date;
	}
	public int getPoint() {
		return eachPoint;
	}
	public int getCombo(){
		return maxCombo;
	}
	public ArrayList<String> getCoworker(){
		return coworkerList;
	}
}
